{
  "name": "creator-ui",
  "version": "0.0.1",
  "description": "shared UI by Creator team",
  "main": "index.ts",
  "type": "module",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean-dist": "rimraf dist/*",
    "build:prod": "cross-env TS_NODE_PROJECT=\"./webpack/tsconfig-for-webpack-config.json\" webpack -p --config=webpack/webpack.prod.ts --progress  --color --display-error-details --bail",
    "build:es6": "tsc && tsc --target es2018 --outDir dist/es6",
    "build": "npm run build:prod",
    "build:dev": "webpack --mode development --config ./webpack.config.js --progress --color --display-error-details",
    "clean": "npm cache clear && rimraf -- dist",
    "server": "npm run server:dev",
    "server:dev": "webpack-dev-server --mode development --config ./webpack.config.js --hot --inline --progress --watch --open",
    "server:prod": "cross-env NODE_ENV=production webpack-dev-server  --mode production --config ./webpack.config.js --port 3000 --host 0.0.0.0 --hot --inline --progress  --watch --open --content-base dist/",
    "start": "npm run server:dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Creator-Eco/creator-ui.git"
  },
  "author": "Or Bachar",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Creator-Eco/creator-ui/issues"
  },
  "homepage": "https://github.com/Creator-Eco/creator-ui#readme",
  "dependencies": {
    "@emotion/core": "^10.0.28",
    "@emotion/css": "^10.0.27",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-dropzone": "^10.2.1",
    "react-select": "^3.1.0",
    "reactjs-popup": "^1.5.0",
    "webpack": "^4.42.0"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "@babel/preset-react": "^7.9.1",
    "@babel/preset-typescript": "^7.9.0",
    "@types/mini-css-extract-plugin": "^0.9.1",
    "@types/node": "^13.9.3",
    "@types/optimize-css-assets-webpack-plugin": "^5.0.1",
    "@types/react": "^16.9.25",
    "@types/terser-webpack-plugin": "^2.2.0",
    "@types/webpack": "^4.41.8",
    "@types/webpack-dev-server": "^3.10.1",
    "@types/webpack-merge": "^4.1.5",
    "@typescript-eslint/eslint-plugin": "^2.24.0",
    "@typescript-eslint/parser": "^2.24.0",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "cross-env": "^7.0.2",
    "eslint": "^6.8.0",
    "mini-css-extract-plugin": "^0.9.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "react-svg-loader": "^3.0.3",
    "rimraf": "^3.0.2",
    "source-map-loader": "^0.2.4",
    "tailwindcss": "^1.2.0",
    "terser-webpack-plugin": "^2.3.5",
    "ts-node": "^8.8.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.8.3",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "webpack-merge": "^4.2.2"
  }
}
